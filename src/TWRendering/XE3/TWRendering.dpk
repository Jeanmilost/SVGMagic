requires
  rtl,
  vcl,
  TWCommon;

contains
  UTWBlur in '..\UTWBlur.pas',
  UTWControlFont in '..\UTWControlFont.pas',
  UTWControlRenderer in '..\UTWControlRenderer.pas',
  UTWGDIPlusGradient in '..\UTWGDIPlusGradient.pas',
  UTWGraphicPath in '..\UTWGraphicPath.pas',
  UTWRenderer in '..\UTWRenderer.pas',
  UTWRenderer_GDI in '..\UTWRenderer_GDI.pas',
  UTWRenderer_GDIPlus in '..\UTWRenderer_GDIPlus.pas',
  UTWRendererCommon in '..\UTWRendererCommon.pas';

end.
